{
    "name": "erickskrauch/composer-exclusive-install",
    "type": "composer-plugin",
    "require": {
        "php": ">=7.1.0",
        "composer-plugin-api": "^1.1",
        "symfony/lock": "^4.3"
    },
    "require-dev": {
        "composer/composer": "^1.8",
        "ely/php-code-style": "^0.3.0"
    },
    "autoload": {
        "psr-4": {
            "ErickSkrauch\\Composer\\ExclusiveInstall\\": "src"
        }
    },
    "extra": {
        "class": "ErickSkrauch\\Composer\\ExclusiveInstall\\Plugin"
    },
    "config": {
        "sort-packages": true
    }
}
